<section
  class="content__modal content__modal--code"
  data-content-tab="code"
  aria-hidden="<%- hidden %>">
  <h1 class="title title--h1">CodeBase</h1>
  <!-- prettier-ignore -->
  <%- include ("../components/text-editor") %>
  <div class="editor">
    <div class="line-numbers" style="display: none">
      <span></span>
    </div>
    <textarea class="code" style="display: none">
      <%- include ("../partials/html-template") %></textarea
    >
  </div>
  <button class="cta cta--theme" data-code-run="">Run</button>
  <iframe src="javascript:void(0);" data-iframe></iframe>
</section>
